    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shikiji": "^0.9.17",
    "pnpm": "^8.14.0",
    "rollup": "^4.9.2",
    "shikiji": "^0.9.17",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
