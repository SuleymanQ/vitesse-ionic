    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.6.0",
    "@capacitor/keyboard": "5.0.7",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@unhead/vue": "^1.8.9",
    "@unocss/reset": "^0.58.3",
    "@vueuse/core": "^10.7.1",
    "@vueuse/head": "^2.0.0",
    "@vueuse/integrations": "^10.7.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vant": "^4.8.2",
