    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.3",
    "@vant/auto-import-resolver": "^1.0.2",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue-macros/volar": "^0.18.8",
    "@vue/test-utils": "^2.4.3",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.56.0",
